{
  "$schema": "https://raw.githubusercontent.com/navidrome/navidrome/refs/heads/new-plugins/plugins/manifest-schema.json",
  "$id": "navidrome://plugins/manifest/brainz",
  "name": "brainz",
  "author": "Kendall Garner",
  "version": "1.0.3",
  "description": "Retrieve metadata from ListenBrainz and ListenBrainz labs API",
  "website": "https://listenbrainz.org",
  "permissions": {
    "http": {
      "reason": "To fetch metadata from listenBrainz (and labs)",
      "allowedHosts": [
        "api.listenbrainz.org",
        "labs.api.listenbrainz.org"
      ]
    }
  }
}